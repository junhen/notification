<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/vector_drawable_bn_fp_to_error"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="fp_to_error"/>
        <Button
            android:id="@+id/vector_drawable_bn_trusted_to_error"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="trusted_to_error" />
        <Button
            android:id="@+id/vector_drawable_bn_fingerprint_draw_on"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="fingerprint_draw_on" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <Button
            android:id="@+id/vector_drawable_bn_error_to_fp"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="error_to_fp" />
        <Button
            android:id="@+id/vector_drawable_bn_error_to_trusted"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="error_to_trusted" />
        <Button
            android:id="@+id/vector_drawable_bn_fingerprint_draw_off"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="fingerprint_draw_off" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:id="@+id/vector_drawable_image"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <View
            android:layout_width="10dp"
            android:layout_height="10dp"/>
        <ImageView
            android:id="@+id/vector_drawable_image_linshi1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/wallpaper_second_icon"/>
        <LinearLayout
            android:id="@+id/vector_drawable_image_linshi2"
            android:layout_width="200dp"
            android:layout_height="100dp"
            android:background="@drawable/wallpaper_second_background"/>
    </LinearLayout>

    <View
        android:layout_width="10dp"
        android:layout_height="10dp"/>
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1px"
        android:alpha="0.2"
        android:background="#FFFFFF"
        android:layout_marginBottom="9dp"/>

</LinearLayout>